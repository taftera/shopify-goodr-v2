@include media-query($small){
  .responsive-table.cart__table{
    .responsive-table-row {
      display: flex;
      flex-wrap: wrap;
      td {
        width: auto;
        clear: none;
        &.cart__item-image {
          //width: 40%;
        }
        &.cart__item-title {
          width: 60%;
        }
        &.cart__item-quanity {
          width: 80%;
        }
        &.cart__item-price {
          width: 20%;
        }
      }
    }
  }
}

.cart__note {
  width: 100%;
}

.order__title {
  font-size: rem-calc(14);
  text-transform: uppercase;
  a {
    color: $color-body-text;
    text-decoration: none;
  }
}
.order__details {
  font-size: rem-calc(14);
  line-height: 1.3em;
  dd {
    padding-bottom: 0.5em;
  }
}


.progressbar__container {
  position: relative;
  border: 1px solid $color-accent;
  width: 100%;
  height: 15px;
  margin-bottom: 10px;
  margin-top: 40px;
}
.progressbar__bar {
  width: 5%;
  height: 100%;
  background: $color-accent;
  position: absolute;
  transition: width 1s ease;
  &:after {
    content: '';
    width: 1.2rem;
    height: 30px;
    background-image: url({{ 'icon_progress_bar.png' | file_img_url: '100x' }});
    background-size: contain;
    background-repeat: no-repeat;
    display: block;
    position: absolute;
    right: -10px;
    bottom: 0;
  }
}
.progressbar__text {
  font-size: rem-calc(10);
  font-family: $font-heading;
  font-weight: $font-heading-weight;
  line-height: 1em;
  margin: 0;
  text-transform: uppercase;
  position: absolute;
  left: 0;
  top: -26px;
  width: 100%;
  > span {
    display: none;
  }
}

.wrap {

  font-family: $font-body;
  font-weight: $font-body-weight;
  font-style: $font-body-style;

  p, span {
    font-family: $font-body;
    font-style: $font-body-style;
    letter-spacing: normal;
  }

  .btn {
    font-family: $font-heading;
    text-transform: uppercase;
    font-weight: bold;
    border-radius: $button-radius;
    padding: 0.7rem 1.5rem;
    font-size: rem-calc(14);
    @include media-query($medium-up){
      font-size: 1rem;
      padding: 0.7rem 2.2rem;
    }
  }

  .btn__content {
    font-family: $font-heading;
    font-weight: bold;
  }

  h2, h3, h4, h5 {
    font-family: $font-heading;
    line-height: 1.1em;
    font-weight: $font-heading-weight;
    line-height: 1.1em;
    color: #11435c;
    text-shadow: none;
    -webkit-text-stroke: transparent;
    text-stroke: transparent;
  }

  .section__title {
    color: #11435c;
    text-shadow: none;
    -webkit-text-stroke: transparent;
    text-stroke: transparent;
    font-size: rem-calc(24);
  }

  .wrap,
  .main,
  .sidebar,
  .sidebar::after {
    background: transparent;
    box-shadow: none;
  }


  .field__input--select {
    padding-right: 35px;
  }

}

.floating-labels .field--show-floating-label .field__label {
  margin-top: 4px;
  line-height: 1em;
}

#storemapper #storemapper-form #storemapper-go {
  background-color: #4ec0b2;
}

#storemapper #storemapper-list li.storemapper-selected {
  background-color: #4ec0b2;
}

/*================ Bundle Builder ================*/
#bundler-app-wrapper {

  .required-notification {
    font-style: italic;
    font-size: 85%;
    opacity: 0.5;
  }
  .bundle-heading {
    text-align: center;
    font-family: "futura-pt-bold","futura",sans-serif;
    color: $color-accent;
    text-shadow: unset;
    text-stroke: unset;
    -webkit-text-stroke: unset;
    padding-top: 2.125rem;
  }

  .bundle-subheading {
    text-align: center;
    font-weight: 700;
    font-size: 1.125rem;
    margin-top: 1.25rem;
    margin-bottom: 0px;
  }
  .product-box {
    margin-bottom: 65px;
  }

  .product {
    display: flex;
    flex-direction: column;
    padding: 30px 1rem 30px 1rem;
    margin: 0;
    overflow: hidden;
    text-align: center;
    position: relative;
    .product-name {
      margin-bottom: 1.5rem;
      font-size: 1.25rem;
      @include media-query($medium-up){
        font-size: 1.75rem
      }
    }
    .bundle-builder--add-to-bundle-form .selector-wrapper.js select {
      width: 80%;
    }
  }

  .bundle-builder--add-to-bundle-form {
    bottom: 0px;
    left: 0px;
    right: 0px;
  }

  .variant-price {
    font-size: 1rem;
    width: 100%;
    line-height: 1.1em;
    margin-bottom: 0.5rem;
  }

  .bundle-content {
    margin: 0 auto;
    font-size: .8rem;
    @media only screen and (min-width: 600px) {
      font-size: 1rem;
    }
    @media only screen and (min-width: 780px) {
      max-width: 65%; 
    }
    .bundle-content-heading {
      font-family: "futura-pt-bold","futura",sans-serif;
      line-height: 1.1em;
      font-weight: bold;
      color: #10435e; 
      text-align: center;
    }
    th:nth-of-type(2) {
      text-align: left; 
    }
    tr:last-child td {
      border-bottom: none; 
    }
    button {
      -webkit-appearance: none;
      border-radius: 0;
      display: inline-block;
      vertical-align: middle;
      width: auto;
      border: unset;
      text-align: center;
      white-space: nowrap;
      text-decoration: none;
      user-select: none;
      appearance: none;
      color: #10435e;
      border-radius: unset;
      padding: unset;
      margin: unset;
      font-family: "futura-pt-bold","futura",sans-serif;
      font-weight: bold;
      font-size: 1rem;
      line-height: 1.1em;
      text-transform: uppercase;
      position: relative;
      background: unset;
      transition: background-color 0.45s cubic-bezier(0.785, 0.135, 0.15, 0.86),color 0.45s cubic-bezier(0.785, 0.135, 0.15, 0.86),border 0.45s cubic-bezier(0.785, 0.135, 0.15, 0.86);
    }
    .selector-wrapper.js select {
      width: 90%;
      margin: 0;
      font-size: inherit;
      @media only screen and (min-width: 600px) {
        width: 80%; 
      }
    }
  }

  .add-bundle-bar.row {
    border-top: 4px solid #e86384;
    justify-content: space-between;
    align-items: center;
    padding: .2rem 2rem;
    margin-top: 2rem;
    flex-direction: column;
    @media only screen and (min-width: 800px) {
      flex-direction: row;
    }
    .bundle-name {
      margin-bottom: 0px; 
    }
    .bundle-price {
      margin-top: 10px; 
      @media only screen and (min-width: 800px) {
        margin-top: 0;
      }
    }
    .error-messages {
      color: red; 
      margin-top: 10px;
      @media only screen and (min-width: 800px) {
        margin-top: 0;
      }
    }
  }

}

/*================ VENDOR ================*/
.giftcard__card {
  position: relative;
  border-radius: 10px;
  border: 1px solid lighten($color-lightgrey,5%);
  max-width: 500px;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: rem-calc(20);
  overflow: hidden;
  line-height: 0;
}
.giftcard__value {
  color: $color-white;
  position: absolute;
  right: 1rem;
  top: 1rem;
  display: inline-block;
  line-height: 1em;
  margin: 0;
}
.giftcard__code {
  background: $color-white;
  font-weight: bold;
  padding: 0.5rem;
  display: inline-block;
  position: absolute;
  bottom: 1rem;
  left: 50%;
  transform: translateX(-50%);
  line-height: 1em;
  font-family: $font-body;
}
.giftcard__qr {
  margin: rem-calc(10) auto;
  padding: rem-calc(10);
  border-radius: 10px;
  border: 1px solid lighten($color-lightgrey,5%);
  display: inline-block;
}
.giftcard__buttons {
  text-align: center;
  position: relative;
  .print-giftcard {
    position: absolute;
    left: 1rem;
    font-weight: bold;
    color: $color-grey;
    text-decoration: none;
  }
}

