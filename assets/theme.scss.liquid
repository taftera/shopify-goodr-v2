/*================ Sliders ================*/
.slick-slider {
  position: relative;
  display: block;
  box-sizing: border-box;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -ms-touch-action: pan-y;
  touch-action: pan-y;
  -webkit-tap-highlight-color: transparent;
}
.slick-list {
  position: relative;
  overflow: hidden;
  display: block;
  margin: 0;
  padding: 0;

  &:focus {
    outline: none;
  }

  &.dragging {
    cursor: pointer;
    cursor: hand;
  }
}
.slick-slider .slick-track,
.slick-slider .slick-list {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.slick-track {
  position: relative;
  left: 0;
  top: 0;
  display: block;

  &:before,
  &:after {
    content: "";
    display: table;
  }

  &:after {
    clear: both;
  }

  .slick-loading & {
    visibility: hidden;
  }
}
.slick-slide {
  float: left;
  height: 100%;
  min-height: 1px;
  [dir="rtl"] & {
    float: right;
  }
  img {
    display: block;
  }
  a {
    border: 0;
  }
  &.slick-loading img {
    display: none;
  }

  display: none;

  &.dragging img {
    pointer-events: none;
  }

  .slick-initialized & {
    display: block;
  }

  .slick-loading & {
    visibility: hidden;
  }

  .slick-vertical & {
    display: block;
    height: auto;
    border: 1px solid transparent;
  }

  &:focus {
    outline: none;
    border: 0;
  }
}
.slick-arrow.slick-hidden {
  display: none;
}
.slick-dots {
  list-style: none;
  margin: 1rem 0;
  padding: 0;
  text-align: center;
  li {
    display: inline-block;
    button {
      background: transparent;
      padding: 0;
      margin: 0 5px;
      width: 15px;
      height: 15px;
      overflow: hidden;
      border-radius: 15px;
      border: 1px solid $color-grey;
      text-indent: -9999px;
      &:active, &:focus {
        outline: none;
        box-shadow: 0 0 1px 1px rgba($color-accent,1);
      }
    }
    &.slick-active {
      button {
        background: $color-grey;
      }
    }
  }
}
.slick-prev,
.slick-next{
  position: absolute;
  top: 50%;
  margin-top: -20px;
  background: transparent;
  border-color: transparent;
  color: $color-accent;
  width: 40px;
  height: 40px;
  padding: 0;
  text-indent: -9999px;
  z-index: 100;
  &:hover {
    background: transparent;
    color: darken($color-accent,15%);
    border-color: transparent!important;
  }
  &:after {
    content: '';
    background-size: 1.5rem;
    background-position: center center;
    background-repeat: no-repeat;
    display: block;
    width: 40px;
    height: 40px;
    position: absolute;
    top: 0;
  }
  &:active, &:focus {
    outline: none;
  }
  &.slick-disabled {
    opacity: 0.3;
  }
}
.slick-prev {
  left: -40px;
  &:after {
    background-image: url({{ 'icon-chevron-left.svg' | asset_url }});
  }
}
.slick-next {
  right: -40px;
  &:after {
    background-image: url({{ 'icon-chevron-right.svg' | asset_url }});
  }
}


