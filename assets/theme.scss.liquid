/* Drawer */

$drawer-width : 360px;

.drawer {
  position: fixed;
  width: $drawer-width;
  height: 100%;
  transform: translateX(0);
  transition: transform 0.4s cubic-bezier(0.645, 0.045, 0.355, 1),
    visibility 0.4s cubic-bezier(0.645, 0.045, 0.355, 1),
    -webkit-transform 0.4s cubic-bezier(0.645, 0.045, 0.355, 1);
  overflow-y: auto;
  overflow-x: hidden;
  background: $color-white;
  padding: 1rem;
  z-index: 2;
}
@media (max-width:360px) {
  .drawer {
    width: 100%;
  }
}

.drawer-close {
  position: absolute;
  right: 1rem;
  top: 1rem;
}

.drawer--left {
  left: -$drawer-width;
}

.drawer--right {
  right: -$drawer-width;
}


.moved-by-drawer {
  transition: opacity 0.2s ease;

}
body.js-drawer-open-right {
  .moved-by-drawer {
    overflow: hidden;
    opacity: 0.5;
  }
  .drawer--right {
    transform: translateX(-$drawer-width);
      box-shadow: 0 0 3px rgba($color-grey,0.3);
      }
}
body.js-drawer-open-left {
  .moved-by-drawer {
    overflow: hidden;
    opacity: 0.5;
  }
  .drawer--left {
    transform: translateX($drawer-width);
    box-shadow: 0 0 3px rgba($color-grey,0.3);
  }
}





