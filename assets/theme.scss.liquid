.nav-top {
  list-style: none;
  margin: 0;
  padding: 0;
}

.nav-top__item {
  display: inline-block;
  > a {
    padding: 2.5rem 0;
    margin: 0 rem-calc(20);
    text-decoration: none;
    font-family: $font-heading;
    font-weight: $font-heading-weight;
    font-size: rem-calc(16);
    text-transform: uppercase;
    color: $color-body-text;
    display: block;
    position: relative;
    &:hover {
      &:after {
        content: '';
        display: block;
        width: 100%;
        border-bottom: 8px solid $color-accent;
        position: absolute;
        bottom: -5px;
        left: 0;
        z-index: 5;
      }
    }
  }
  &:first-child {
    a {
      margin-left: 0;
    }
  }
  > ul,
  .nav-top__mega-dropdown {
    display: none;
  }
  &:hover {
    > ul,
    .nav-top__mega-dropdown {
      display: block;
    }
  }
}

.nav-top__mega-dropdown {
  position: absolute;
  left: 0;
  width: 100vw;
  background: $color-body;
  padding-top: 1rem;
  padding-bottom: 1rem;
  border-top: 4px solid $color_secondary;
  z-index: 6;
  ul {
    list-style: none;
    li {
      margin: 0;
      ul {
        list-style: none;
        margin: 0;
        padding: 0;
      }
    }
  }
}

.nav-top__mega-dropdown-child {
  width: auto;
  display: inline-block;
  margin-right: rem-calc(70);
  vertical-align: top;
}

