    margin: 0;
  }
}

.nav-mobile {
  list-style: none;
  margin: 1rem 0;
  padding: 0;
  a {
    display: block;
  }
  .nav-link--parent {
    padding: rem-calc(12) 0;
  }
  ul {
    list-style: none;
    margin: 0;
    padding: 0;
    border-top: 1px solid $color-border;
    li {
      position: relative;
      border-bottom: 1px solid $color-border;
      .nav-link--child {
        margin-left: 0;
        padding: rem-calc(12) rem-calc(15);
      }
    }
    ul {
      padding-left: 0;
      padding-bottom: rem-calc(12);
      display: none;
      border: 0;
      li {
        border: 0;
        .nav-link--grandchild {
          margin-left: 0;
          padding-left: rem-calc(15);
        }
      }
    }
  }
}

