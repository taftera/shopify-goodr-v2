// Global filters
.filter-menu {
  border: 0;
  .filter-group {
    h4 {
      font-family: $font-heading;
      font-weight: $font-heading-weight;
      text-transform: uppercase;
      color: $color-body-text;
      letter-spacing: 0.02em;
      border-bottom: 0;
      font-size: 1rem;
      cursor: pointer;
    }
  }

  .pt-filter-mode-and .has_selected .active,
  .filter-group .has_selected .inactive {
    display: inline-block;
  }

  ul.has_selected li a
  {
    opacity: 0.7;
  }
  ul.has_selected li.selected a {
    opacity: 1;
  }

  .has_group_selected .filter-active-standard:hover .collection-name .check-icon {
    opacity: 0;
  }


  .filter-clear {
    border-radius: $button-radius;
    background: $color-accent;
    display: inline-block;
    color: $color-white;
    text-transform: uppercase;
  }

  ul,
  ul.has_selected {
    list-style: none;
    margin: rem-calc(30) 0;
    text-align: center;
    display: block;
    li,
    li.selected {
      font-family: $font-body;
      font-weight: $font-heading-weight;
      text-transform: uppercase;
      color: $color-body-text;
      display: inline-block;
      width: auto!important;
      margin: 0;
      padding: 10px 10px;
      text-align: center;
      position: relative;
      &:not(:disabled){
        cursor: pointer;
      }
      a {
        margin: 0;
        padding: 0;
        text-indent: 0;
      }
    }
    li.inactive {
      display: inline-block;
    }
  }

  ul.has_selected {
    li.selected {
      &:after {
        content: '';
        display: block;
        position: absolute;
        left: 10px;
        top: 10px;
        background-color: $color-accent;
        background-image: url({{ 'icon-check.svg' | asset_url }});
        background-repeat: no-repeat;
        background-size: 80% 80%;
        background-position: center center;
        color: $color-white;
        border-radius: 20px;
        width: 15px;
        height: 15px;
      }
    }
  }

  .filter-active-big .collection-name .check-icon {
    opacity: 1;
  }
}


// Mobile filters
@include media-query($small){
  .filter-menu {
    .pt-mobile-header {
      text-align: left;
      height: 38px;
      a.pt-nav-toggle {
        border: 1px solid $color-accent;
        padding: 0.5em;
        line-height: 1em;
        transform: translate(-50%, 7px);
        p {
          font-family: $font-heading;
          font-weight: $font-heading-weight;
          text-transform: uppercase;
          font-size: rem-calc(14)!important;
          color: $color-grey;
          top: auto;
          line-height: 1em;
          margin: 0;
          &:after {
            content: '+';
            display: inline-block;
            font-family: $font-heading;
            font-weight: $font-heading-weight;
            margin-left: rem-calc(10);
            color: $color-accent;
          }
        }
        &.active {
          p:after {
            content: '-';
          }
        }
        span,
        span:before,
        span:after {
          display: none;
        }
      }
    }

    .has_group_selected {
      h4,
      .menu-trigger,
      .scroll-content {
        display: none;
      }
    }
    &.pt-expand {
      .has_group_selected {
        h4,
        .menu-trigger,
        .scroll-content {
          display: block;
        }
      }
    }

  }


}
// Click/desktop filters
@include media-query($medium-up){
  .filter-menu {
    margin-bottom: 0;

    .filter-clear {
      padding: 0.5em 1em;
      top: 1rem;
      right: 4rem;
    }
  }

  .filter-menu .filter-group {
    padding: 0;
    margin: 0;
    display: inline-block;
    position: static;
    min-width: 0;

    h4 {
      padding: rem-calc(20) rem-calc(15);
      margin: 0;
      cursor: pointer;
      position: relative;
      &:before {
        content: '';
        display: block;
        width: 1px;
        height: 70%;
        border-right: 1px solid $color-body-text;
        position: absolute;
        top: 15%;
        right: 0;
        z-index: 5;
      }
      &:hover {
        &:after {
          content: '';
          display: block;
          width: 100%;
          border-bottom: 3px solid $color-secondary;
          position: absolute;
          bottom: -2px;
          left: 0;
          z-index: 11;
        }
      }
    }

    &:last-child {
      h4:before {
        border-right: 0;
      }
    }

    .scroll-content {
      position: absolute;
      width: 100vw;
      left: 0;
      background: $color-white;
      z-index: 10;
      border-top: 1px solid $color-accent;
      border-bottom: 4px solid $color-secondary;
      display: none;
    }

  }
}
