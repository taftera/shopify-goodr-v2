	text-align: center;
}
.klaviyo_form_box_inner {
	padding: 20px 20px 10px;
	text-align: center;
	border: 1px solid #fff;
}
.klaviyo_form_box .klaviyo_form_title {
    color: #fff;
    text-shadow: none;
    -webkit-text-stroke: unset;
    text-transform: uppercase;
    font-size: 28px;
    font-family: "futura-pt-bold";
  	position: relative;
  	display: inline-block;
	padding: 20px 50px 30px;
}
.klaviyo_form_box .klaviyo_form_title:after {
    content: "";
  	background-image: url({{ 'balloons-right.png' | file_img_url: 'master' }});
  	display: inline-block;
	background-size: cover;
	background-repeat: no-repeat;
	margin-left: 10px;
	width: 40px;
	height: 92px;
	position: absolute;
  	right: 0;
  	top: 0;
}
.klaviyo_form_box .klaviyo_form_title:before {
    content: "";
  	background-image: url({{ 'balloons-left.png' | file_img_url: 'master' }});
  	display: inline-block;
	background-size: cover;
	background-repeat: no-repeat;
	margin-right: 10px;
	width: 40px;
	height: 92px;
